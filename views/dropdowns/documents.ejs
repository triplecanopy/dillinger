<li
  ng-repeat="document in documents track by document.id"
  ng-class="{
    'active': currentDocument.id === document.id,
    'octocat': document.isGithubFile
    }"
  >
  <a class="document--remove" ng-click="removeDocument(document)">
    <span class="glyphicon glyphicon-trash"></span>
  </a>
  <a class="document--title" ng-click="selectDocument(document)">{{document.title}}</a>
</li>
